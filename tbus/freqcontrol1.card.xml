  <IntegerParameter name="PID I" min="-32768" max="32767" defaultvalue="0" /> -->

  <IntegerParameter name="GateTime" msb="15" lsb="0" defaultvalue="0" /> in units of 1us
  <IntegerParameter name="FifoTimebaseMultiplier" max="8191" min="0" defaultvalue="0" /> number of samples-1 of counter data to average over in Fifo-mode
  <IntegerParameter name="FifoCounterShift" max="7" min="0" defaultvalue="0" /> number of bits to shift right the summed counter output in fifo-mode
  
  <IntegerParameter name="Modulation phase,mod_phase" min="0" max="255" defaultvalue="0" /> in units of 10ns
  <IntegerParameter name="Modulation frequency,mod_freq" defaultvalue="0" min="0" max="7"/> 0=50MHz, 1=25MHz, 2=12.5MHz, 3=6.25MHz, 4=3.125MHz, 5=1.5625MHz, 6=781kHz, 7=391kHz
  <FloatParameter name="Modulation amplitude,mod_ampl" defaultvalue="0.0" min="0" max="0.999938965" OnUnderflow="clip" OnOverflow="clip" abs="on" />

  <IntegerParameter name="Beat Divider 1,BeatDiv1" min="0" max="3" defaultvalue="0" /> 0=div8, 1=div4, 2=div4, 3=div2
