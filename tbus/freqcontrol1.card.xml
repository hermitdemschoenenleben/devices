  <IntegerParameter name="Beat Divider 3,BeatDiv3" min="0" max="3" defaultvalue="0" />
  <BooleanParameter name="BeatDiv2_1" defaultvalue="false" />
  <BooleanParameter name="BeatDiv2_2" defaultvalue="false" />
  <BooleanParameter name="BeatDiv2_3" defaultvalue="false" />
  
  <IntegerParameter name="Count1" min="0" max="65535" defaultvalue="0" />read back the frequency counter 1
